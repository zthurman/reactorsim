record(ai, "pressurizer:level")
{
    field(DESC, "Pressurizer Level")
    field(LINR, LINEAR)
    field(EGUF, 100)
    field(EGUL, 0)
    field(EGU, "%")
    field(HIGH, 90)
    field(HSV, MINOR)
}
record(calc, "pressurizer:level:trip")
{
    field(DESC, "Pressurizer Level Trip")
    field(CALC, "(A>0)?B:C")
    field(SCAN, "1 second")
    field(INPA, "pressurizer:level.STAT")
    field(INPB, 0)
    field(INPC, 1)
}
record(ai, "pressurizer:pressurizer")
{
    field(DESC, "Pressurizer Pressure")
    field(LINR, LINEAR)
    field(EGUF, 2500)
    field(EGUL, 1700)
    field(EGU, PSIG)
    field(HIGH, 2375)
    field(HSV, MINOR)
    field(LOW, 1925)
    field(LSV, MAJOR)
}
record(calc, "pressurizer:pressure:trip")
{
    field(DESC, "Pressurizer Pressure Trip")
    field(CALC, "(A>0)?B:C")
    field(SCAN, "1 second")
    field(INPA, "pressurizer:pressure.STAT")
    field(INPB, 0)
    field(INPC, 1)
}
record(calc, "pressurizer:trip")
{
    field(DESC, "Pressurizer Trip Aggregate")
    field(CALC, "A&B")
    field(SCAN, "1 second")
    field(INPA, "pressurizer:pressure:trip")
    field(INPB, "pressurizer:level:trip")
}
